{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "quotemark": [true, "single", "jsx-double"],
        "object-literal-sort-keys": false,
        "jsx-no-multiline-js": false,
        "jsx-no-lambda": false,
        "max-line-length": false
    },
    "rulesDirectory": []
}